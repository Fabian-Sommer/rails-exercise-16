<p>
  <strong>Title:</strong>
  <%= @paper.title %>
</p>

<p>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</p>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>
<p>
	Authors<br>
	<% @paper.authors.each do |author| %>
	<%= author.name %><br>
	<% end %>
</p>

<%= link_to 'Destroy', paper_path(@paper),
method: :delete,
data: { confirm: 'Are you sure?' } %>
<%= link_to 'Back', papers_path %>