<h1>Listing papers</h1>
<table>
  <tr>
    <th>Name</th>
	<th>Venue</th>
    <th>Year</th>
  </tr>  
  
  <% @papers.each do |paper| %>
  <tr>

    <td>
      <%= link_to paper.title, paper_path(paper) %>
    </td>
	<td>
      <%= paper.venue %>
    </td>
    <td>
      <%= paper.year %>
    </td>
    <td>
      <%= link_to 'Show', paper_path(paper) %>
    </td>
    <td>
      <%= link_to 'Edit', edit_paper_path(paper) %>
    </td>
    <td>
      <%= link_to 'Destroy', paper_path(paper), method: :delete, data: { confirm: 'Are you sure?' } %>
	  <a href="/papers/ <%= paper.id %> /delete">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
<%= link_to 'Add paper', new_paper_path %>

|

  <%= link_to 'Back', root_path %>